<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <!-- Necessário para que o EF reconheça a seção 'entityFramework' -->
  <configSections>
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" />
  </configSections>
  <!-- String de conexão para o banco MySQL -->
  <connectionStrings>
    <add name="BancoInvestimentos" connectionString="server=localhost;port=3306;database=db_corretora;uid=root;pwd=@Rodrigues2012;" providerName="MySql.Data.MySqlClient" />
  </connectionStrings>
  <!-- Configuração do Entity Framework 6 com MySQL -->
  <entityFramework>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.LocalDbConnectionFactory, EntityFramework">
      <parameters>
        <parameter value="v12.0" />
      </parameters>
    </defaultConnectionFactory>
    <providers>
      <provider invariantName="MySql.Data.MySqlClient" type="MySql.Data.MySqlClient.MySqlProviderServices, MySql.Data.Entity" />
    </providers>
  </entityFramework>
